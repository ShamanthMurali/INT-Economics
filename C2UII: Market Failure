        <mxCell id="iTKbe5SUne0fed-IwmjX-66" value="Private goods" style="rounded=1;whiteSpace=wrap;html=1;" parent="1" vertex="1">
          <mxGeometry x="2065" y="160" width="120" height="40" as="geometry" />
        </mxCell>
        <mxCell id="3dlbmDzRoKMXdMmCx6VQ-1" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;" parent="1" source="iTKbe5SUne0fed-IwmjX-67" target="iTKbe5SUne0fed-IwmjX-73" edge="1">
