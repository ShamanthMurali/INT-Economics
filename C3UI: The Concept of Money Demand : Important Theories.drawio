        <mxCell id="JGfa3QQ2Kr3XQhtsew2E-24" value="&lt;div&gt;1. MV = PT&lt;/div&gt;&lt;div&gt;2.`MV + M^1V^1 = PT`&lt;br&gt;&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
